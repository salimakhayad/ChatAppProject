@model ChatApp.Models.Home.HomeModel
@{
    ViewData["Title"] = "Home Page";

}
    <div class="container">
        <h2>Profielen <i class="fas fa-angle-double-right"></i> Index</h2>
        @{
            <div>
                <ul class="hs">
                    @foreach (var profile in Model.Profielen)
                    {
                        <partial name="_ProfielDetails" model="profile" />
                    }
                </ul>
            </div>
        }
        <h2>ChatGroups <i class="fas fa-angle-double-right"></i> Index</h2>
        @{
            <div>
                <ul class="hs">
                    @foreach (var chatGroup in Model.ChatGroups)
                    {
                        <partial name="_ProfielDetails" model="chatGroup" />
                    }
                </ul>
            </div>
        }
    </div>
